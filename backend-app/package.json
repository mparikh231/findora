{
  "name": "backend-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "db:generate": "npx drizzle-kit generate --config=./db/drizzle/migrations/drizzle.config.js",
    "db:migrate": "npx drizzle-kit migrate --config=./db/drizzle/migrations/drizzle.config.js",
    "db:push": "npx drizzle-kit push --config=./db/drizzle/migrations/drizzle.config.js"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "drizzle-orm": "0.35.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "drizzle-kit": "0.27.0",
    "nodemon": "^3.1.11"
  }
}
