    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "drizzle-orm": "^0.44.7",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.5",
    "nodemon": "^3.1.10"
  }
}
